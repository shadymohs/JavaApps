COA.TRANSACTION_SEQ ,
COA.REMARKS ,
ST.LOCALE_NAME AR_STATUS, ST.FOREIGN_NAME  EN_STATUS ,
COA.USER_ID ,
TO_CHAR(COA.CREATED_DATE, 'YYYY-MM-DD HH24:MI') DATE_TIME

from LUGGAGE_COMPLAINT_ACTIONS COA
left join LOST_LUGGAGE_STATUS ST ON COA.STATUS = ST.LOST_LUGGAGE_STATUS_ID
LEFT JOIN LOST_LUGGAGE_COMPLAINT CO ON COA.LUGGAGE_COMPLAINT_ID=CO.LUGGAGE_COMPLAINT_ID

where COA.LUGGAGE_COMPLAINT_ID = $P{compId}

      ORDER BY  COA.TRANSACTION_SEQ asc]]>
